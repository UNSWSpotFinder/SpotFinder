    "definitions": {
        "Manager.createManagerRequestData": {
            "type": "object",
            "required": [
                "adminID",
                "name"
            ],
            "properties": {
                "adminID": {
                    "type": "string",
                    "example": "123456"
                },
                "name": {
                    "type": "string",
                    "example": "longsizhuo"
                },
                "password": {
                    "type": "string",
                    "example": "123456"
